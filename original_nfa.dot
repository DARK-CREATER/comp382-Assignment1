digraph N3_original {
    rankdir=LR;
    node [shape=circle];

    start [shape=point];  
    q0 [label="q0 (start)"];
    q1 [label="q1"];
    q2 [label="q2"];
    q3 [label="q3"];
    q4 [label="q4"];

    # epsilon transition
    start -> q0 [label="ε"];
    start -> q3 [label="ε"];

    # two-cycle (even lengths)
    q0 -> q1 [label="0"];
    q1 -> q0 [label="0"];

    # three-cycle (multiples of 3)
    q3 -> q4 [label="0"];
    q4 -> q2 [label="0"];
    q2 -> q3 [label="0"];

    # accepting states
    q0 [shape=doublecircle];
    q3 [shape=doublecircle];
}